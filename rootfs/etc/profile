# /etc/profile: system-wide .profile file for the Bourne shells

# Ash profile
# vim: syntax=sh

# No core files by default
#ulimit -S -c 0 > /dev/null 2>&1

USER="'id -un'"
LOGNAME=$USER
PS1='[\u@\h \W]# '
PATH=$PATH

HOSTNAME='/bin/hostname'

export USER LOGNAME PS1 PATH

echo
echo -n "Processing /etc/profile... "
# no-op
echo "Done"
echo

